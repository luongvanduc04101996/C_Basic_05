#include<stdio.h>
void main(){
int n,k,i,s=0;
printf("Nhap 2 so:\n");
scanf("%d%d",&n,&k);
int KiemTra(int a,int b)
{
        if(a%b==0)
            return 1;
        else
            return 0;
}
for(i=1;i<n;i++)
{
    if(KiemTra(i,k)==1)
        s++;
    else
        continue;
}
printf("Cac so be hon %d co %d so chia het cho %d",n,s,k);
}

